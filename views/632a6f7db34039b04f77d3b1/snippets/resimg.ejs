<div class="media <%= classwrapper %>" <% if(image){ %>style="padding-bottom: <%= 1 / image.aspect_ratio * 100 %>%;"<% } %>>
    <img srcset="<% if(image.width >= 165){ %><%= image | img_url: '165x' %> 165w,<% } %>
        <% if(image.width >= 360){ %><%= image | img_url: '360x' %> 360w,<% } %>
        <% if(image.width >= 535){ %><%= image | img_url: '535x' %> 535w,<% } %>
        <% if(image.width >= 720){ %><%= image | img_url: '720x' %> 720w,<% } %>
        <% if(image.width >= 940){ %><%= image | img_url: '940x' %> 940w,<% } %>
        <% if(image.width >= 1070){ %><%= image | img_url: '1070x' %> 1070w<% } %>"
        src="<%= image | img_url: '535x' %>"
        sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
        alt="<%= image.alt %>"
        
        width="<%= image.width %>"
        height="<%= image.height %>"
        class="<%= classimg %>"
    />
</div>