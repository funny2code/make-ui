<section id="shopify-section-header">
  
<link rel="stylesheet" href="/assets/component-list-menu.css">
<link rel="stylesheet" href="/assets/component-search-style-1.css">
<link rel="stylesheet" href="/assets/component-menu-drawer.css">
<link rel="stylesheet" href="/assets/component-cart-notification-style-1.css">
<link rel="stylesheet" href="/assets/component-header-style-1.css">

<style>
  :root {
    --header-py_header_bg_color_1: <%= section.settings.py_header_bg_color_1 %>; 
    --header-py_header_color: <%= section.settings.py_header_color %>;
    --header-py_header_link_hov_color_1: <%= section.settings.py_header_link_hov_color_1 %>;
    --header-py_header_link_color_1: <%= section.settings.py_header_link_color_1 %>;
    --header-py_header_link_hov_color_1: <%= section.settings.py_header_link_hov_color_1 %>;
    --header-py_header_link_color_2: <%= section.settings.py_header_link_color_2 %>;
    --header-py_header_link_hov_color_2: <%= section.settings.py_header_link_hov_color_2 %>;
    --header-py_header_bg_color_2: <%= section.settings.py_header_bg_color_2 %>;
    --header-py_search_bg_color: <%= section.settings.py_search_bg_color %>;
    --header-py_search_color: <%= section.settings.py_search_color %>;
    --header-py_search_hover_bg_color: <%= section.settings.py_search_hover_bg_color %>;
    --header-py_search_hover_color: <%= section.settings.py_search_hover_color %>;
    --header-ck_btn_bg: <%= settings.ck_btn_bg %>;
    --header-ck_btn_color: <%= settings.ck_btn_color %>;
    --header-ck_btn_hover_bg: <%= settings.ck_btn_hover_bg %>;
    --header-ck_btn_hover_color: <%= settings.ck_btn_hover_color %>;
    --header-item_title_font_size: <%= settings.item_title_font_size %>;
    --header-item_price_font_size: <%= settings.item_price_font_size %>;
    --header-item_content_font_size: <%= settings.item_content_font_size %>;
    --header-button_radius: <%= settings.button_radius %>;
    --header-item_bg: <%= settings.item_bg %>;
    --header-item_radius: <%= settings.item_radius %>;
  }

  .header-wrapper,.header__submenu,
  .search-modal.modal__content,.menu-drawer,.menu-drawer__submenu{
    background: var(--header-py_header_bg_color_1);
  }
  .bc-mega-menu-image__title,.bc-mega-menu__title{
    color: var(--header-py_header_color);
  }
  .menu-drawer__menu-item--active,
  .menu-drawer__menu-item:focus,
  .menu-drawer__close-button:focus,
  .menu-drawer__menu-item:hover,
  .menu-drawer__close-button:hover {
    color: var(--header-py_header_link_hov_color_1);
  }
  .header__heading *,
  .top-left-nav-item,
  .header__icons *,
  .header-wrapper a,
  .list-menu__item,
  .header-wrapper .link--text{
    color: var(--header-py_header_link_color_1);
  }
  .header__heading *:hover,
  .top-left-nav-item:hover,
  .header__icons>*:hover *,
  .header-wrapper a:hover,
  .list-menu__item:hover{
    color: var(--header-py_header_link_hov_color_1);
  }
  .header--top-left .header__inline-menu .list-menu__item,
  .header--top-left-with-nav .header__inline-menu .list-menu__item{
    color: var(--header-py_header_link_color_2);
  }
  .header--top-left .header__inline-menu .list-menu__item:hover,
  .header--top-left-with-nav .header__inline-menu .list-menu__item:hover{
    color: var(--header-py_header_link_hov_color_2);
  }
  .header--top-left .header__submenu,
  .header--top-left-with-nav .header__submenu{
    background:  var(--header-py_header_bg_color_2);
  }
  .header-search-container .header__icon--search form .search__input.field__input{
    border-radius: 30px;
    background:  var(--header-py_search_bg_color);
    box-shadow: 0 0 0 0.1rem #EDEDED;
    border-bottom: 0px;
  }
  .search__button svg{
    height: 1.8rem;
    width: 1.8rem;
  }
  .header-search-container .header__icon--search form .search__input.field__input,
  .header-search-container .header__icon--search form .field__label,
  .header-search-container .header__icon--search form .search__button svg{
    color: var(--header-py_search_color);
  }
  .header-search-container .header__icon--search form .search__input.field__input:hover{
    background:  var(--header-py_search_hover_bg_color);
  }
  .header-search-container .header__icon--search form:hover .search__input.field__input,
  .header-search-container .header__icon--search form:hover .field__label,
  .header-search-container .header__icon--search form:hover .search__button svg{
    color: var(--header-py_search_hover_color);
  }
  .header--top-left .header__inline-menu{
    padding-top: 5px;
  }
  @media(min-width:990px){
    .header--top-left-with-nav,.header--top-left{
      padding:0px;
    }
    .header--top-left.py-pd-y-xl{
      padding-top:var(--py-pd-y-xl);
    }
    .header--top-left.py-pd-y-l{
      padding-top:var(--py-pd-y-l);
    }
    .header--top-left.py-pd-y-m{
      padding-top:var(--py-pd-y-m);
    }
    .header--top-left.py-pd-y-s{
      padding-top:var(--py-pd-y-s);
    }
    .header--top-left.py-pd-y-xs{
      padding-top:var(--py-pd-y-xs);
    }
    .header--top-left.py-pd-x-xl .header__heading,
    .header--top-left-with-nav.py-pd-x-xl .header__heading{
      padding-left:var(--py-pd-x-xl);
    }
    .header--top-left.py-pd-x-l .header__heading,
    .header--top-left-with-nav.py-pd-x-l .header__heading{
      padding-left:var(--py-pd-x-l);
    }
    .header--top-left.py-pd-x-m .header__heading,
    .header--top-left-with-nav.py-pd-x-m .header__heading{
      padding-left:var(--py-pd-x-m);
    }
    .header--top-left.py-pd-x-s .header__heading,
    .header--top-left-with-nav.py-pd-x-s .header__heading{
      padding-left:var(--py-pd-x-s);
    }
    .header--top-left.py-pd-x-xs .header__heading,
    .header--top-left-with-nav.py-pd-x-xs .header__heading{
      padding-left:var(--py-pd-x-xs);
    }
    .header--top-left.py-pd-x-xl .header__icons,
    .header--top-left-with-nav.py-pd-x-xl .header__icons{
      padding-right:var(--py-pd-x-xl);
    }
    .header--top-left.py-pd-x-l .header__icons,
    .header--top-left-with-nav.py-pd-x-l .header__icons{
      padding-right:var(--py-pd-x-l);
    }
    .header--top-left.py-pd-x-m .header__icons,
    .header--top-left-with-nav.py-pd-x-m .header__icons{
      padding-right:var(--py-pd-x-m);
    }
    .header--top-left.py-pd-x-s .header__icons,
    .header--top-left-with-nav.py-pd-x-s .header__icons{
      padding-right:var(--py-pd-x-s);
    }
    .header--top-left.py-pd-x-xs .header__icons,
    .header--top-left-with-nav.py-pd-x-xs .header__icons{
      padding-right:var(--py-pd-x-xs);
    }
    .header--top-left .header__inline-menu,
    .header--top-left-with-nav .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
    }
    .header--top-left.py-pd-x-xl .header__inline-menu,
    .header--top-left-with-nav.py-pd-x-xl .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
      padding-left: var(--py-pd-x-xl);
      padding-right: var(--py-pd-x-xl);
    }
    .header--top-left.py-pd-x-l .header__inline-menu,
    .header--top-left-with-nav.py-pd-x-l .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
      padding-left: var(--py-pd-x-l);
      padding-right: var(--py-pd-x-l);
    }
    .header--top-left.py-pd-x-m .header__inline-menu,
    .header--top-left-with-nav.py-pd-x-m .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
      padding-left: var(--py-pd-x-m);
      padding-right: var(--py-pd-x-m);
    }
    .header--top-left.py-pd-x-s .header__inline-menu,
    .header--top-left-with-nav.py-pd-x-s .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
      padding-left: var(--py-pd-x-s);
      padding-right: var(--py-pd-x-s);
    }
    .header--top-left.py-pd-x-xs .header__inline-menu,
    .header--top-left-with-nav.py-pd-x-xs .header__inline-menu{
      background:  var(--header-py_header_bg_color_2);
      padding-left: var(--py-pd-x-xs);
      padding-right: var(--py-pd-x-xs);
    }
    .header--top-left .header__inline-menu{
      margin-top:8px;
    }
    .top-left-nav-tabs{
      display:flex;
    }
    .top-left-nav-item{
      cursor: pointer;
      font-weight: 600;
    }
    .top-left-nav-item {
        padding: 20px;
    }
    .header--top-left-with-nav.py-pd-y-xl .top-left-nav-item{
      padding:var(--py-pd-y-xl) 20px;
    }
    .header--top-left-with-nav.py-pd-y-l .top-left-nav-item{
      padding:var(--py-pd-y-l) 20px;
    }
    .header--top-left-with-nav.py-pd-y-m .top-left-nav-item{
      padding:var(--py-pd-y-m) 20px;
    }
    .header--top-left-with-nav.py-pd-y-s .top-left-nav-item{
      padding:var(--py-pd-y-s) 20px;
    }
    .header--top-left-with-nav.py-pd-y-xs .top-left-nav-item{
      padding:var(--py-pd-y-xs) 20px;
    }
    .top-left-nav-item.active{
      background: var(--header-py_header_bg_color_2);
      color: var(--header-py_header_link_color_2);
    }
  }
  @media (max-width:989px){
    .header-top-left-nav-container,
    .header-search-container{
      display:none;
    }
  }
  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }
  body.index .header-position-fixed{
    position:absolute;
    width:100%;
  }
  body.index .header-position-fixed,
  body.index .header-position-fixed .header__inline-menu,
  body.index .header-position-fixed .top-left-nav-item.active{
    background-color:transparent !important;
  }
  body.index .header-position-fixed .header-search-container{
    visibility: hidden;
  }
  #loopcheckout{
    background-color: var(--header-ck_btn_bg);
    color: var(--header-ck_btn_color);
  }
  #loopcheckout:hover{
    background-color: var(--header-ck_btn_hover_bg);
    color: var(--header-ck_btn_hover_color);
  }
  .bc-cart__item .bc-cart__title{
    font-size: var(--header-item_title_font_size);
  }
  .bc-cart__item .bc-cart__variant-title{
    font-size: var(--header-item_title_font_size);
  }
  .bc-cart__item .bc-cart__price-wrapper{
    font-size:  var(--header-item_price_font_size);
  }
  .bc-cart__item .bc-cart__properties-wrapper{
    font-size: var(--header-item_content_font_size);
  }
</style>
<% if(settings.button_radius){ %>
  <style>
    .bc-cart__buttons-wrapper .button{
      border-radius:  var(--header-button_radius)px;
    }
  </style>
<% } %>
<% if(settings.item_bg){ %>
  <style>
  .bc-cart__item{
    padding:10px;
    background-color: var(--header-item_bg);
    border-radius: var(--header-item_radius)px;
  }
  </style>
<% } %>
<% if(settings.item_radius){ %>
<style>
  .bc-cart__item .bc-cart__image-wrapper img{
    border-radius:  var(--header-item_radius)px;
  }
</style>
<% } %>
<% if(settings.enable_dynamic_pay){ %>
<style>
  #loopcheckoutdynamic{
    margin-top: 15px;
    background: #5b32f6;
  }
  #loopcheckoutdynamic:hover{
    box-shadow: 0 0 10px black;
  }
  #loopcheckoutdynamic img{
    width: 74px;
  }
</style>
<% } %>

<script src="/assets/details-disclosure.js" defer="defer"></script>
<script src="/assets/details-modal.js" defer="defer"></script>

<svg xmlns="http://www.w3.org/2000/svg" class="hidden">
  <symbol id="icon-search" viewbox="0 0 18 19" fill="none">
    <path fill-rule="evenodd" clip-rule="evenodd" d="M6.5 0C8.22391 0 9.87721 0.684819 11.0962 1.90381C12.3152 3.12279 13 4.77609 13 6.5C13 8.11 12.41 9.59 11.44 10.73L11.71 11H12.5L17.5 16L16 17.5L11 12.5V11.71L10.73 11.44C9.59 12.41 8.11 13 6.5 13C4.77609 13 3.12279 12.3152 1.90381 11.0962C0.684819 9.87721 0 8.22391 0 6.5C0 4.77609 0.684819 3.12279 1.90381 1.90381C3.12279 0.684819 4.77609 0 6.5 0ZM6.5 2C4 2 2 4 2 6.5C2 9 4 11 6.5 11C9 11 11 9 11 6.5C11 4 9 2 6.5 2Z" fill="currentColor"/>
