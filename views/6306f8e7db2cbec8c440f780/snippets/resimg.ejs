<% if(image){ %>
<div class="<%= classwrapper %>">
    <img srcset="<%= image.includes('/') ? image : `/${srcId}/${image}` %> 165w,
        <%= image.includes('/') ? image : `/${srcId}/${image}` %> 360w,
        <%= image.includes('/') ? image : `/${srcId}/${image}` %> 535w,
        <%= image.includes('/') ? image : `/${srcId}/${image}` %> 720w,
        <%= image.includes('/') ? image : `/${srcId}/${image}` %> 940w,
        <%= image.includes('/') ? image : `/${srcId}/${image}` %> 1070w"
        src="<%= image.includes('/') ? image : `/${srcId}/${image}` %>"
        sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
        class="<%= classimg %>"
    />
</div>
<% } %>