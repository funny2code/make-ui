<header class="py__header-random">
    <div class="py__header-random-wrapper">
        <div class="py__header-random-logo">
            <a href="/">
                <img src="/images/logo.png" alt="<%=make.title%>" width="120"/>
            </a>
        </div>
        <div class="py__header-random-buttons">
            <% if(pageNames){ %>
                <div class="py__preview-select">
                    <select class="py__preview-pages-select">
                        <% pageNames.forEach(item => { %> 
                            <option data-href="<% if(user && !isAdmin){ %>/users/<%=user._id%><% } %>/remix/<%=id%>?page=<%=item.name%>" <% if(page === item.name){ %>selected<% } %>>
                                <%=item.name%>
                            </option>
                        <% }) %>
                    </select>
                </div>
            <% } %> 
            <a href="#" class="py__button py__button-black py__button-random">
                <span class="py__button-svg">
                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="19" viewBox="0 0 22 19" fill="none">
                    <path d="M15.8867 0.5L21.1367 5L15.8867 9.5L21.1367 14L15.8867 18.5V15.5H13.1467L10.3267 12.68L12.4467 10.56L14.3867 12.5H15.8867V9.5V6.5H14.3867L5.38672 15.5H0.886719V12.5H4.14672L13.1467 3.5H15.8867V0.5ZM0.886719 3.5H5.38672L8.20672 6.32L6.08672 8.44L4.14672 6.5H0.886719V3.5Z" fill="white"/>
                    </svg>
                </span>
                <span class="py__button-label">Remix Style</span>
            </a>
        </div>
    </div>
</header>