<% if(image){ %>
<div class="<%= classwrapper %>">
    <img srcset="/images/<%= image %> 165w,
    /images/<%= image %> 360w,
    /images/<%= image %> 535w,
    /images/<%= image %> 720w,
    /images/<%= image %> 940w,
    /images/<%= image %> 1070w"
        src="/images/<%= image %>"
        sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
        class="<%= classimg %>"
    />
</div>
<% } %>