<link rel="stylesheet" href="/<%=srcId%>/component-rte.css" media="print" onload="this.media='all'">
<link rel="stylesheet" href="/<%=srcId%>/section-main-page.css" media="print" onload="this.media='all'">

<div class="page-width">
  <h2 class="page-title">
    <% if(section.settings.page?.title){ %>
      <%= section.settings.page?.title %>
    <% } else { %>
      Page title
    <% } %>
  </h2>
  <div class="rte">
    <% if(section.settings.page?.content){ %>
      <%- section.settings.page?.content -%>
    <% } else { %>
      <div class='page-placeholder-wrapper placeholder'>
        <%# 'page' | placeholder_svg_tag: 'page-placeholder' %>
        Page Content
      </div>
    <% } %>
  </div>
</div>