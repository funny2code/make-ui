<div id="<%=wrapperId%>" class="media <%= classwrapper %>" <% if(image){ %>style="padding-bottom: <%= 1 / image.aspect_ratio || 1 * 100 %>%;"<% } %>>
    <img srcset="<% if(image.width >= 165){ %><%= image %> 165w,<% } %>
        <% if(image.width >= 360){ %><%= image %> 360w,<% } %>
        <% if(image.width >= 535){ %><%= image %> 535w,<% } %>
        <% if(image.width >= 720){ %><%= image %> 720w,<% } %>
        <% if(image.width >= 940){ %><%= image %> 940w,<% } %>
        <% if(image.width >= 1070){ %><%= image %> 1070w<% } %>"
        src="<%= image %>"
        sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
        alt="<%= image.alt %>"
        
        width="<%= image.width %>"
        height="<%= image.height %>"
        class="<%= classimg %>"
    />
</div>